<!DOCTYPE html>
<html>
  <head>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  
    <title>	Movie App </title>
    <% include head %>
  </head>
  <body>
  
    <% include navbar %>
  
    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          <h1>Now Playing</h1>
        </div>
        <div class="col-sm-12">
          <% parsedData.forEach((movie) => { %>
            <div class="col-md-3 col-sm-4 poster">
              <a href="/movie/<%= movie.id %>"><img src="<%= imageBaseUrl + movie.poster_path %>"></a>
              <h2><%= movie.title %></h2>
            </div>
          <% }) %>
          
        </div>
      </div>
    </div>
  </body>
  </html>
